{
  "name": "Singleton (Light)",
  "style": "light",
  "author": {
    "name": "Adam Kiss",
    "github": "https://github.com/adamkiss",
    "website": "https://adamkiss.com"
  },
  "editor": {
    "backgroundColor": "#ffffff",
    "altBackgroundColor": "#e9ecf6",
    "toolbarBackgroundColor": "#f3f5fa",
    "toolbarIconColor": "#002bd6",
    "tintColor": "#002bd6",
    "tintColor2": "#002bd6",
    "textColor": "#000000",
    "menuItemColor": "#e9ecf6",
    "shouldOverwriteFont": false
  },
  "styles": {
    "body": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "font": ".AppleSystemUIFont",
      "fontSize": 16,
      "color": "#000000"
    },
    "tabbed": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "headIndent": 36
    },
    "title1": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 8,
      "font": ".AppleSystemUIFontBold",
      "size": 20,
      "color": "#484669"
    },
    "title-mark1": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 8,
      "size": 20,
      "color": "#66484669"
    },
    "title2": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "font": ".AppleSystemUIFontBold",
      "size": 18,
      "color": "#1c2e05",
      "backgroundColor": "#eefccb"
    },
    "title-mark2": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "size": 18,
      "color": "#661c2e05",
      "backgroundColor": "#eefccb"
    },
    "title3": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "font": ".AppleSystemUIFontDemi",
      "size": 18,
      "color": "#000000"
    },
    "title-mark3": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "size": 18,
      "color": "#66000000"
    },
    "title4": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "font": ".AppleSystemUIFont",
      "size": 18,
      "color": "#000000"
    },
    "title-mark4": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "size": 18,
      "color": "#66000000"
    },
    "bold": {
      "font": ".AppleSystemUIFontDemi"
    },
    "bold-left-mark": {},
    "bold-right-mark": {},
    "italic": {
      "type": "italic",
      "font": ".AppleSystemUIFontItalic"
    },
    "italic-left-mark": {},
    "italic-right-mark": {},
    "boldItalic": {
      "font": ".AppleSystemUIFontDemiItalic",
      "underlineStyle": 0
    },
    "boldItalic-left-mark": {},
    "boldItalic-right-mark": {},
    "strikethrough": {
      "color": "#7974b7",
      "strikethroughStyle": 2,
      "strikethroughColor": "#7974b7"
    },
    "strikethrough-left-tilde": {
      "color": "#cc7974b7"
    },
    "strikethrough-right-tilde": {
      "color": "#cc7974b7"
    },
    "underline": {
      "color": "#ff0000",
      "underlineStyle": 2,
      "underlineColor": "#ff0000"
    },
    "highlighted": {
      "color": "#002bd6",
      "font": ".AppleSystemUIFont",
      "backgroundColor": "#332865ff",
      "rightBorderPadding": 2
    },
    "highlighted-left-marker": {
      "color": "#990034ff"
    },
    "highlighted-right-marker": {
      "color": "#990034ff"
    },
    "quote-mark": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "headIndent": 36,
      "font": "noteplanstate",
      "color": "#337974b7"
    },
    "quote-content": {
      "headIndent": 36,
      "color": "#6862a0"
    },
    "code": {
      "font": "FragmentMono-Regular",
      "backgroundColor": "#66e9ecf6"
    },
    "code-left-backtick": {
      "isHiddenWithoutCursor": false,
      "isRevealOnCursorRange": false,
      "color": "#cc7974b7",
      "backgroundColor": "#66e9ecf6"
    },
    "code-right-backtick": {
      "isHiddenWithoutCursor": false,
      "isRevealOnCursorRange": false,
      "color": "#cc7974b7",
      "backgroundColor": "#66e9ecf6"
    },
    "code-fence": {
      "theme": "grayscale",
      "corner-radius": 6,
      "body": {
        "font": "FragmentMono-Regular",
        "size": 15
      },
      "language-specifier": {
        "color": "#7f000000",
        "lineSpacing": 4,
        "paragraphSpacingBefore": 0,
        "paragraphSpacing": 4
      },
      "fence-open": {
        "color": "#33000000"
      },
      "fence-close": {
        "color": "#33000000"
      }
    },
    "todo": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "headIndent": 36,
      "font": "noteplanstate",
      "color": "#4f92ff"
    },
    "checked": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "headIndent": 36,
      "color": "#000000"
    },
    "checked-char": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "headIndent": 36,
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\] )",
      "matchPosition": 0,
      "color": "#cc00bd0d"
    },
    "checked-char-canceled": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "headIndent": 36,
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
      "matchPosition": 0,
      "color": "#b28c8cc8"
    },
    "checked-char-scheduled": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "headIndent": 36,
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
      "matchPosition": 0,
      "color": "#b28c8cc8"
    },
    "working-on": {
      "headIndent": 36,
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "backgroundColor": "#00ffffff"
    },
    "flagged-1": {
      "headIndent": 36,
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "backgroundColor": "#334f92ff"
    },
    "flagged-2": {
      "headIndent": 36,
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "backgroundColor": "#00ffffff"
    },
    "flagged-3": {
      "headIndent": 36,
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "backgroundColor": "#00ffffff"
    },
    "link": {
      "color": "#cc0034ff"
    },
    "note-title-link-right-bracket": {
      "isHiddenWithoutCursor": false,
      "isRevealOnCursorRange": false
    },
    "note-title-link-left-bracket": {
      "isHiddenWithoutCursor": false,
      "isRevealOnCursorRange": false
    },
    "hashtag": {
      "rightBorderPadding": 2
    },
    "attag": {
      "rightBorderPadding": 2
    },
    "TimeBlock_Project_Link": {
      "lineSpacing": 4,
      "paragraphSpacingBefore": 0,
      "paragraphSpacing": 4,
      "regex": "(\\+\\[\\[.*?\\]\\])(?:\\s+)",
      "color": "#002bd6"
    },
    "TimeBlock_Time": {
      "regex": "(\\d{1,2}[H|h|m|M])",
      "matchPosition": 1,
      "font": ".AppleSystemUIFontDemi"
    },
    "TimeBlock_Text": {
      "regex": "(\\+\\[\\[.*?\\]\\])(?:\\s+)\\s?(\\d{1,2}[H|h|m|M]){1,2}\\s?(.*)$",
      "matchPosition": 3,
      "color": "#7974b7"
    }
  }
}